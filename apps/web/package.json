    "classnames": "^2.3.1",
    "framer-motion": "^6.3.3",
    "next": "12.1.6",
    "next-themes": "^0.1.1",
    "react": "^18.0.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.30.0",
    "react-iconly": "^2.2.5",
    "react-intl": "^5.25.1",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "17.0.31",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "babel-plugin-formatjs": "^10.3.20",
    "config": "*",
    "eslint": "^8.14.0",
