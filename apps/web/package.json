{
  "name": "@dew-org/web",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "predev": "npm run i18n:compile",
    "dev": "next dev",
    "prebuild": "npm run i18n:compile",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
