  },
  "dependencies": {
    "@formatjs/cli": "^4.8.3",
    "@nextui-org/react": "^1.0.6-beta.5",
    "next": "12.0.8",
    "next-themes": "^0.1.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-hook-form": "^7.29.0",
    "react-intl": "^5.24.8"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@types/react": "17.0.37",
    "babel-plugin-formatjs": "^10.3.19",
    "config": "*",
    "eslint": "^8.12.0",
    "next-transpile-modules": "9.0.0",
    "tsconfig": "*",
    "typescript": "^4.5.3"
  }
}
